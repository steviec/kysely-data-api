{
  "name": "kysely-data-api",
  "version": "0.0.11",
  "repository": "git@github.com:serverless-stack/kysely-data-api.git",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist && rm -rf test/dist",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && ./module-fixup.sh",
    "build:esm": "tsc -p tsconfig-esm.json",
    "build:cjs": "tsc -p tsconfig-cjs.json",
    "format": "eslint --fix .",
    "lint": "tsc --noEmit && eslint .",
    "test": "AWS_SDK_LOAD_CONFIG=1 jest",
    "prepare": "npm run build"    
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.0",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "aws-sdk": "^2.1008.0",
    "esbuild": "^0.13.6",
    "esbuild-runner": "^2.2.1",
    "eslint": "^8.16.0",
    "jest": "^27.2.5",
    "kysely": "^0.19.3",
    "perf_hooks": "^0.0.1"
  },
  "peerDependencies": {
    "aws-sdk": "^2.1008.0",
    "kysely": "^0.19.3"
  },
  "files": [
    "dist"
  ]
}
